{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ping": {
    "auth": {
      "IdentityUrl": "https://localhost:44379",
      "login_endpoint": "/Login?licenceOrName={0}&prenom={1}",
      "logout_endpoint": "/Logout?token={0}"
    },
    "api": {
      "EndPoint": "http://localhost:56903",
      "Club": "/api/clubs/{numero}",
      "JoueursDuClub": "/api/joueurs/club/{numero}",
      "Joueur": "/api/joueurs/{licence}",
      //"ReloadJoueursDuClub": "/api/joueurs/club/{numero}/load",
      "PartiesDuJoueur": "/api/joueurs/{licence}/parties",
      "HistoriquesDuJoueur": "/api/joueurs/{licence}/historique",
      "JoueurExtra": "/api/joueurs/{licence}/extra",
      "HistoriqueClassementDuJoueur": "/api/joueurs/{licence}/histoclass",
      "HistoriquePointDuJoueur": "/api/joueurs/{licence}/histopoint",
      "HistoriqueVictoireDuJoueur": "/api/joueurs/{licence}/histovictoire",
      "HistoriqueDefaiteDuJoueur": "/api/joueurs/{licence}/histodefaite",
      "EquipesDuclub": "/api/clubs/{numero}/equipes/resultats",
      "Organismes": "/api/organismes/force",
      "ResultatEquipe": "/api/championnats/{division}/{poule}/resultat",
      "ClassementEquipe": "/api/championnats/{division}/{poule}/classement"
    }
  }
  
}
